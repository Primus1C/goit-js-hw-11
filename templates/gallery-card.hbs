{{#each this}}
<div class="photo-card">
    <img class="photo-card-img" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
    <div class="info">
        <p class="info-title">Likes</p>
        <p class="info-item">
            <b>{{likes}}</b>
        </p>
        <p class="info-title">Views</p>
        <p class="info-item">
            <b>{{views}}</b>
        </p>
        <p class="info-title">Comments</p>
        <p class="info-item">
            <b>{{comments}}</b>
        </p>
        <p class="info-title">Downloads</p>
        <p class="info-item">
            <b>{{downloads}}</b>
        </p>
    </div>
</div>
{{/each}}